@using EntityLayer.Concrete
@model List<About>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Hakkımda Veri Listesi</h2>
<table class="table table-bordered">
    <tr>
        <th>Id</th>
        <th>Hakkımda Başlığı</th>
        <th>Aktif yap</th>
        <th>Pasif Yap</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.AboutId</td>
            <td>başşlık</td>
            <td><a href="#" class="btn btn-success">Aktif yap</a></td>
            <td><a href="#" class="btn btn-danger">Pasif yap</a></td>
        </tr>
    }
</table>

@Html.Action("AboutPartial", "About")